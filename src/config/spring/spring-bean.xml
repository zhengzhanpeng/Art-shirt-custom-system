<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                            http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
    <bean id="returnMessage" class="java.util.HashMap">
        <constructor-arg>
            <map key-type="java.lang.String" value-type="java.lang.String">
                <entry key="LOGIN_ERROR" value="您的权限有误，请于管理员联系"/>
                <entry key="USER_NAME_OR_PASSWORD_WRONG" value="您的用户名或密码不正确，请重新输入"/>
                <entry key="SYSTEM_ERROR" value="系统繁忙，请稍后再试或与管理员联系"/>
                <entry key="USER_NAME_NOT_NULL" value="用户名不能为空"/>
                <entry key="NAME_NOT_NULL" value="姓名不能为空"/>
                <entry key="USER_NAME_NOT_NULL" value="用户名不能为空"/>
                <entry key="DEPARTMENT_NOT_NULL" value="部门不能为空"/>
                <entry key="PASSWORD_NOT_NULL" value="密码不能为空"/>
                <entry key="AUTHORITY_NOT_NULL" value="用户权限不能为空"/>
                <entry key="PHONE_NOT_NULL" value="手机号不能为空"/>
            </map>
        </constructor-arg>
    </bean>
</beans>